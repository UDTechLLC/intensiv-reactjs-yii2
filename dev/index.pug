
//- var imagePath = "https://projects.udtech.co/intensivkurs/assets/images/"
- var imagePath = "assets/images/"
include mixins/plan_tab


doctype html
html(lang="en", class="loading")
	head
		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible")

		block title
			title intensivkurs

		meta(name="description", content="")
		meta(name="viewport", content="width=device-width,initial-scale=1")

		block styles
			//-main styles
			link(rel="stylesheet", href="assets/css/libs.css")
			link(rel="stylesheet", href="assets/css/app.css")

	body
		.page(id="app")
			// block HEADER
			.header.flex
				.map(id="map-layout")
					//img(src="" + imagePath + "map.jpg", alt="map")
				.filter-section(:class="{'open-school-view': schoolViewStatus}")
					.flex.flex-center.header-filter
						a(href="#").b_logo
							img(src= imagePath + "logo.svg", alt="Intensivkurs Logo")
					.body
						h2
							span NÄRHET
							| KVALITET & MILJÖ

						form(method="POST", action="#", v-on:submit.prevent="openSchoolView")
							.drive
								h3 Välj ett fordon
								.section.flex
									.button-license.b
										input(type="radio", name="license", value="b", v-model="pickedLicense", v-on:change="changeLicense")
										.wrap-btn
											.icon
												img(src= imagePath + "b.svg", alt="B Logo", class="svg")
											|B
										.dropdown.animated.fadeIn
											input(type="radio", name="license", value="ub", id="license-ub", v-model="pickedLicense", v-on:change="changeLicense")
											label(for="license-ub")
												span UB
												|Bil + Släpvagn (Max 4.25 ton)
											input(type="radio", name="license", value="be", id="license-be", v-model="pickedLicense", v-on:change="changeLicense")
											label(for="license-be")
												span BE
												|Tung släpvagn (Max 3.5 ton)
									.button-license.a
										input(type="radio", name="license", value="a", v-model="pickedLicense", v-on:change="changeLicense")
										.wrap-btn
											.icon
												img(src= imagePath + "a.svg", alt="A Logo", class="svg")
											|A
										.dropdown.animated.fadeIn
											input(type="radio", name="license", value="a1", id="license-a1", v-model="pickedLicense", v-on:change="changeLicense")
											label(for="license-a1")
												span A1
												|Lätt motorcykel (Max 11 Kw)
											input(type="radio", name="license", value="a2", id="license-a2", v-model="pickedLicense", v-on:change="changeLicense")
											label(for="license-a2")
												span A2
												|Mellanstor motorcykel (Max 35 Kw)
									.button-license.am
										input(type="radio", name="license", value="am", v-model="pickedLicense", v-on:change="changeLicense")
										.wrap-btn
											.icon
												img(src= imagePath + "am.svg", alt="AM Logo", class="svg")
											|AM
							.home
								h3 Välj en ort eller en trafikskola
								.section

									select2(name="search_area_map", class="select-contain", v-model="selectPlace")
										option(disabled, selected, value="placeholder") Välj ort
										option(v-for="place in listPlaces", :value="place.id") {{place.city}}

									select2(name="search_name", class="select-contain", v-model="selectSchool")
										option(disabled, selected, value="placeholder") Välj trafikskola
										option(v-for="school in listSchools", :value="school.id") {{school.name}}

									script(type="text/x-template", id="select2-template")
										select
											slot
							input(type="submit" value="Välj en trafikskola", class="btn-blue")
				.school-view
