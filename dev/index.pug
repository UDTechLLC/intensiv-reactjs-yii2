						.row
							.col.col-sm-5
								p.label Pedagogik
							.col.col-sm-7
								.block-statistic
									.progress-bar.pedagogical
						.row
							.col.col-sm-5
								p.label Miljöfordon
							.col.col-sm-7
								.block-statistic
									.progress-bar.cleanVehicles
						.row
							.col.col-sm-5
								p.label Rekommendation
							.col.col-sm-7
