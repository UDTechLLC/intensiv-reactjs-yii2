
- var imagePath = "assets/images/"

doctype html
html(lang="en")
	head
		meta(charset="utf-8")
		meta(http-equiv="X-UA-Compatible")

		block title
			title intensivkurs

		meta(name="description", content="")
		meta(name="viewport", content="width=device-width,initial-scale=1")

		block styles
			//-main styles
			link(rel="stylesheet", href="assets/css/libs.css")
			link(rel="stylesheet", href="assets/css/app.css")

	body
		// block HEADER
		.header.flex(id="app")
			.map
				img(src="" + imagePath + "map.jpg", alt="map")
			.filter-section(:class="{'open-school-view': schoolViewStatus}")
				.flex.flex-center.header-filter
					a(href="#").b_logo
						img(src="/" + imagePath + "logo.svg", alt="Intensivkurs Logo")
				.body
					h2 NÄRHET KVALITET & MILJÖ

					form(method="POST", action="#", v-on:submit.prevent="openSchoolView")
						.drive
							h3 Välj ett fordon
							.section.flex
								.button-license.b
									input(type="radio", name="license", value="b")
									.wrap-btn B
									.dropdown.animated.fadeIn
										input(type="radio", name="license", value="ub", id="license-ub")
										label(for="license-ub")
											span UB
											|släpvagn (Max 4.25 ton)
										input(type="radio", name="license", value="be", id="license-be")
										label(for="license-be")
											span BE
											|tung släpvagn (Max 3.5 ton)
								.button-license.a
									input(type="radio", name="license", value="a")
									.wrap-btn A
									.dropdown.animated.fadeInDown
										input(type="radio", name="license", value="a1", id="license-a1")
										label(for="license-a1")
											span A1
											|Lätt motorcykel (Max 11 Kw)
										input(type="radio", name="license", value="a2", id="license-a2")
										label(for="license-a2")
											span A2
											|Mellanstor motorcykel (Max 35 Kw)
								.button-license.am
									input(type="radio", name="license", value="am")
									.wrap-btn AM
						.home
							h3 Välj en ort eller en trafikskola
							.section

								select2(name="search_area_map")
									option(disabled, selected, value="placeholder") Välj ort
									option(value="1") Bromma
									option(value="3") Botkyra
									option(value="4") Danderyd-Täby
									option(value="5") Farsta
									option(value="6") Haninge
									option(value="7") Huddinge
									option(value="8") Hässelby-Vällingby
									option(value="9") Jakobsberg
									option(value="10") Lidingö
									option(value="11") Nacka
									option(value="12") Norrtälje
									option(value="13") Nynäshamn
									option(value="14") Sollentuna
									option(value="15") Sundbyberg/Solna
									option(value="16") Södermalm
									option(value="17") Södertälje
									option(value="18") Upplands Väsby
									option(value="19") Tyresö
									option(value="20") Västermalm
									option(value="21") Östermalm

								select2(name="search_name", v-model="selectSchool")
									option(disabled, selected, value="placeholder") Välj trafikskola
									option(v-for="school in listSchools", :value="school.id") {{school.name}}

								script(type="text/x-template", id="select2-template")
									select
										slot
						input(type="submit" value="Välj en trafikskola", class="btn-blue")
			.school-view
				button.close(v-on:click="closeSchoolView")
				h2 {{schoolView.name}}
				p.description Kvalitet och miljörevision IKS-17
				.graph

				.table-statistic
					.row
						.col.col-sm-5
							p.label Kundsupport
						.col.col-sm-7
							.block-statistic
								.progress-bar.support
					.row
						.col.col-sm-5
							p.label Lärarlegitimation
						.col.col-sm-7
							.block-statistic
								.progress-bar.registrationTeacher
					.row
						.col.col-sm-5
							p.label Pedagogik
						.col.col-sm-7
							.block-statistic
								.progress-bar.pedagogical
					.row
						.col.col-sm-5
							p.label Miljöfordon
						.col.col-sm-7
							.block-statistic
								.progress-bar.cleanVehicles
					.row
						.col.col-sm-5
							p.label Rekommendation
						.col.col-sm-7
							.block-statistic
								.progress-bar.recommendation
				.contact-info
					.info-block.location {{schoolView.address}}
					.info-block.email {{schoolView.email}}
					.info-block.phone {{schoolView.phone}}
					input.btn-blue(type="submit", value="Välj en trafikskola")

